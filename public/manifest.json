{
    "name": "Wikishift",
    "description": "Reposition Wikipedia Table of Content",
    "version": "0.0.0.2",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["js/content.js"],
        "css": ["css/style.css"]
    }],
    "browser_action": {
        "default_title": "Make it Rain"
    },
    "icons": {
		"16": "images/16x16.png",
		"32": "images/32x32.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
    },
    "manifest_version": 2
}
